<table sqlname="order_info"  >
    <!--不新增的字段-->
    <not_insert_fields>raw_update_time</not_insert_fields>
    <!-- 不能修改的字段 -->
    <not_update_fields>id,gid,serial_no,service_name,service_version,partner_id,charset,raw_add_time,request_params
    </not_update_fields>
    <!--不做为查询条件的字段-->
    <not_query_fields>charset,raw_add_time,raw_update_time,request_params,result_message</not_query_fields>


    <column name="status" javatype="com.jvv.openapi.facade.constants.enums.Status" typeHandler="com.jvv.openapi.dal.handler.StatusTypeHandler"/>

</table>